{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods": ["GET", "POST"],
            "headers": {
                "Access-Control-Allow-Origin": "https://auth-app-awka.vercel.app/",
                "Access-Control-Allow-Credentials": "true"
            }
        }
    ]
}
