{
    "builds": [
        {
            "src": "index.js",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/index.js",
            "methods": ["GET", "POST"],
            "hearders": {
                "Access-Control-Allow-Origin": "*"
            }
        }
    ]
}
